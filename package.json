{
  "name": "ngx-duration-picker-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "test_lib": "ng test ngx-duration-picker",
    "test_lib_ci": "ng test ngx-duration-picker --watch=false --no-progress --browsers=ChromeHeadlessCI",
    "e2e": "ng e2e",
    "e2e_ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "build_lib": "ng build ngx-duration-picker",
    "copy_license": "cp ./LICENSE ./dist/ngx-duration-picker/",
    "copy_readme": "cp ./README.md ./dist/ngx-duration-picker/",
    "copy_files": "npm run copy_license && npm run copy_readme",
    "npm_pack": "cd dist/ngx-duration-picker && npm pack",
    "package": "npm run build_lib && npm run copy_files && npm run npm_pack",
    "all_checks": "npm run package && npm run test_lib_ci && npm run e2e_ci"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.0",
    "@angular/common": "^7.1.0",
    "@angular/compiler": "^7.1.0",
    "@angular/core": "^7.1.0",
    "@angular/forms": "^7.1.0",
    "@angular/platform-browser": "^7.1.0",
    "@angular/platform-browser-dynamic": "^7.1.0",
    "@angular/router": "^7.1.0",
    "core-js": "^2.5.7",
    "rxjs": "^6.3.3",
    "tslib": "^1.9.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.0",
    "@angular-devkit/build-ng-packagr": "^0.11.0",
    "@angular/cli": "^7.1.0",
    "@angular/compiler-cli": "^7.1.0",
    "@angular/language-service": "^7.1.0",
    "@types/node": "^10.12.10",
    "@types/jasmine": "^3.3.0",
    "@types/jasminewd2": "^2.0.6",
    "bootstrap": "^4.1.3",
    "codelyzer": "^4.5.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.4.0",
    "protractor": "^5.4.1",
    "ts-node": "^7.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  }
}
