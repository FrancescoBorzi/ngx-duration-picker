{
  "name": "ngx-duration-picker-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:demo": "npm run build_lib && ng build --configuration production --base-href .",
    "test:integration": "ng test",
    "test:unit": "ng test ngx-duration-picker",
    "test:integration:ci": "ng test --watch=false --no-progress --browsers=ChromeHeadlessCI",
    "test:unit:ci": "ng test ngx-duration-picker --watch=false --no-progress --browsers=ChromeHeadlessCI",
    "test:integration:ci:firefox": "ng test --watch=false --no-progress --browsers=FirefoxHeadless",
    "test:unit:ci:firefox": "ng test ngx-duration-picker --watch=false --no-progress --browsers=FirefoxHeadless",
    "lint": "ng lint",
    "build_lib": "ng build ngx-duration-picker --configuration production",
    "copy_license": "cp ./LICENSE ./dist/ngx-duration-picker/",
    "copy_readme": "cp ./README.md ./dist/ngx-duration-picker/",
    "copy_files": "npm run copy_license && npm run copy_readme",
    "npm_pack": "cd dist/ngx-duration-picker && npm pack",
    "package": "npm run build_lib && npm run copy_files && npm run npm_pack",
    "all_checks": "npm run package && npm run lint && npm run test:unit:ci && npm run test:integration:ci && npm run test:unit:ci:firefox && npm run test:integration:ci:firefox"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.2",
    "@angular/common": "12.2.2",
    "@angular/compiler": "12.2.2",
    "@angular/core": "12.2.2",
    "@angular/forms": "12.2.2",
    "@angular/platform-browser": "12.2.2",
    "@angular/platform-browser-dynamic": "12.2.2",
    "@angular/router": "12.2.2",
    "rxjs": "6.6.7",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.2",
    "@angular-devkit/core": "12.2.2",
    "@angular/cli": "12.2.2",
    "@angular/compiler-cli": "12.2.2",
    "@angular/language-service": "12.2.2",
    "@types/jasmine": "3.8.2",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "14.17.10",
    "bootstrap": "4.6.0",
    "codelyzer": "6.0.2",
    "jasmine-core": "3.9.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-firefox-launcher": "2.1.1",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "ng-packagr": "12.2.0",
    "ts-node": "10.2.1",
    "tslint": "6.1.3",
    "typescript": "4.3.5"
  }
}
