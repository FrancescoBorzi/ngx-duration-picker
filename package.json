{
  "name": "ngx-duration-picker-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "test_lib": "ng test ngx-duration-picker",
    "test_lib_ci": "ng test ngx-duration-picker --watch=false --no-progress --browsers=ChromeHeadlessCI",
    "e2e": "ng e2e",
    "e2e_ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "build_lib": "ng build ngx-duration-picker",
    "copy_license": "cp ./LICENSE ./dist/ngx-duration-picker/",
    "copy_readme": "cp ./README.md ./dist/ngx-duration-picker/",
    "copy_files": "npm run copy_license && npm run copy_readme",
    "npm_pack": "cd dist/ngx-duration-picker && npm pack",
    "package": "npm run build_lib && npm run copy_files && npm run npm_pack",
    "all_checks": "npm run package && npm run test_lib_ci && npm run e2e_ci"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.1.3",
    "@angular/common": "8.1.3",
    "@angular/compiler": "8.1.3",
    "@angular/core": "8.1.3",
    "@angular/forms": "8.1.3",
    "@angular/platform-browser": "8.1.3",
    "@angular/platform-browser-dynamic": "8.1.3",
    "@angular/router": "8.1.3",
    "rxjs": "6.5.2",
    "tslib": "1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.3",
    "@angular-devkit/build-ng-packagr": "~0.801.3",
    "@angular/cli": "8.1.3",
    "@angular/compiler-cli": "8.1.3",
    "@angular/language-service": "8.1.3",
    "@types/jasmine": "3.3.16",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.6.8",
    "bootstrap": "4.3.1",
    "codelyzer": "^5.0.1",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.2.0",
    "karma-chrome-launcher": "3.0.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "ng-packagr": "^5.1.0",
    "protractor": "5.4.2",
    "ts-node": "8.3.0",
    "tsickle": "^0.35.0",
    "tslint": "5.18.0",
    "typescript": "~3.4.5"
  }
}
