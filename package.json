{
  "name": "ngx-duration-picker-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "test_lib": "ng test ngx-duration-picker",
    "test_lib_ci": "ng test ngx-duration-picker --watch=false --no-progress --browsers=ChromeHeadlessCI",
    "e2e": "ng e2e",
    "e2e_ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "build_lib": "ng build ngx-duration-picker",
    "copy_license": "cp ./LICENSE ./dist/ngx-duration-picker/",
    "copy_readme": "cp ./README.md ./dist/ngx-duration-picker/",
    "copy_files": "npm run copy_license && npm run copy_readme",
    "npm_pack": "cd dist/ngx-duration-picker && npm pack",
    "package": "npm run build_lib && npm run copy_files && npm run npm_pack",
    "all_checks": "npm run package && npm run test_lib_ci && npm run e2e_ci"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.2.5",
    "@angular/common": "10.2.5",
    "@angular/compiler": "10.2.5",
    "@angular/core": "10.2.5",
    "@angular/forms": "10.2.5",
    "@angular/platform-browser": "10.2.5",
    "@angular/platform-browser-dynamic": "10.2.5",
    "@angular/router": "10.2.5",
    "rxjs": "6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular-devkit/build-ng-packagr": "~0.1002.3",
    "@angular/cli": "10.2.3",
    "@angular/compiler-cli": "10.2.5",
    "@angular/language-service": "10.2.5",
    "@types/jasmine": "3.5.11",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "^12.11.1",
    "bootstrap": "4.5.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "ts-node": "8.10.2",
    "tslint": "~6.1.0",
    "typescript": "4.0.8"
  }
}
